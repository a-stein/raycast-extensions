{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ensk-is",
  "title": "Ensk.is",
  "description": "Search the Ensk.is English to Icelandic dictionary",
  "icon": "command-icon.png",
  "author": "jokull",
  "categories": [
    "Productivity",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "English to Icelandic",
      "subtitle": "Ensk.is",
      "description": "Find the Icelandic translation for an English word",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.0",
    "better-sqlite3": "12.1.1",
    "fuse.js": "6.6.2",
    "node-fetch": "^3.0.0",
    "sql.js": "1.7.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/better-sqlite3": "7.5.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/sql.js": "1.4.3",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "typescript": "^4.4.3"
  },
  "overrides": {
    "tar-fs": ">=3.1.1"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  },
  "platforms": [
    "macOS"
  ]
}
