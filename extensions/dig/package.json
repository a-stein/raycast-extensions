{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dig",
  "title": "Dig - DNS Lookup",
  "description": "DNS lookup and nameserver query using the built-in host command",
  "icon": "command-icon.png",
  "author": "danielbahl",
  "categories": [
    "Developer Tools"
  ],
  "contributors": [
    "pernielsentikaer",
    "j3lte"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Query Hostname",
      "subtitle": "dig",
      "description": "Returns NS-records from the hostname",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.3",
    "@raycast/utils": "^2.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.37.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS"
  ]
}
